{
  "name": "Subhub",
  "version": "1",
  "manifest_version": 2,
  "description": "Add an 'Open in Sublime' button to GitHub",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "permissions": [],
   "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
        "lib/jquery.js", 
        "scripts/content.js"
      ]
    }
  ]
}
